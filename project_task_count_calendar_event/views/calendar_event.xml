<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="act_calendar_event_meetings_line_by_project" model="ir.actions.act_window">
        <field name="name">Activities</field>
        <field name="res_model">calendar.event</field>
        <field name="view_type">form</field>
        <field name="view_mode">calendar,form</field>
        <field name="view_id" ref="calendar.view_calendar_event_calendar"/>
        <field name="domain">[('project_id', '!=', False)]</field>
        <field name="context">{"default_project_id": active_id, "search_default_project_id": [active_id]}</field>
        <field name="search_view_id" ref="calendar.view_calendar_event_search"/>
    </record>

</odoo>