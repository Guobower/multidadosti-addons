<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="wizard_calendar_event_form" model="ir.ui.view">
        <field name="name">wizard.calendar.event.form</field>
        <field name="model">wizard.calendar.event</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_edit_only">
                        <label for="meeting_duration"/>
                        <field name="meeting_duration" widget="float_time"
                               class="oe_inline"/>
                        <span> hours</span>
                    </div>

                    <notebook>
                        <page string="Meeting Feedback">
                            <field name="meeting_feedback" style="margin: 10px 0px 10px 0px;"/>
                        </page>
                    </notebook>
                </sheet>
                <footer>
                    <button name="finish" string="Finish" type="object"
                            default_focus="1" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>